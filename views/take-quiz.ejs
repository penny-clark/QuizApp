<%- include('header') %>

    <main>

      <h3>quiz(title)</h3>
      <p class="question">
        <span>Q.</span> question(content) What material is Captain America's shield made of?
      </p>

      <form action="" method="POST" id="take-quiz-form">
        <div class="form-inline-wrap">
          <input type="radio" id="possiblea1" name="pickanswer" value="answer1">
          <label for="take_quiz_option1">answer1 Vibranium</label>
        </div>
        <div class="form-inline-wrap">
          <input type="radio" id="possiblea2" name="pickanswer" value="answer2">
          <label for="take_quiz_option2">answer2 Galvorn</label>
        </div>

        <div>
          <button type="submit">Submit answer</button>
        </div>
      </form>

    </main>

  </body>
</html>
